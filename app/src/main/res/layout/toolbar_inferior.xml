<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/primary">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent">

        <ImageView
            android:id="@+id/toolbarIconoCasa"

            android:src="@drawable/ic_baseline_home_24"
            app:tint="@color/white"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/toolbarIconEjercicio"
            app:layout_constraintTop_toTopOf="parent"
             />

        <ImageView
            android:id="@+id/toolbarIconEjercicio"

            android:src="@drawable/ic_baseline_fitness_center_24"
            app:tint="@color/white"
            android:padding="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            app:layout_constraintStart_toEndOf="@id/toolbarIconoCasa"
            app:layout_constraintEnd_toStartOf="@id/toolbarIconoLigas"
            app:layout_constraintTop_toTopOf="parent"
            />

        <ImageView
            android:id="@+id/toolbarIconoLigas"

            android:src="@drawable/ic_baseline_workspace_premium_24"
            app:tint="@color/white"


            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="15dp"
            app:layout_constraintStart_toEndOf="@id/toolbarIconEjercicio"
            app:layout_constraintEnd_toStartOf="@id/toolbarIconoComida"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/toolbarIconoComida"

            android:src="@drawable/ic_baseline_food_bank_24"
            app:tint="@color/white"


            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="15dp"
            app:layout_constraintStart_toEndOf="@id/toolbarIconoLigas"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>


    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>